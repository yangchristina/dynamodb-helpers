{
  "compilerOptions": {
      "outDir": "./dist",
      "target": "es6",
      "lib": ["esnext", "es2018"],
      "module": "Node16",
      "esModuleInterop": true,
      "baseUrl": ".",
      "moduleResolution": "node16",
      "sourceMap": true,
      "declaration": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "skipLibCheck": true,
      "experimentalDecorators": true,
  },
}
